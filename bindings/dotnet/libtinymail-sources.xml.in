<gapi-parser-input>

  <api filename="libtinymail-api.raw">
    <library name="libtinymail-@API_VERSION@.dll">
      <namespace name="Tny">
        <directory path="../../libtinymail/">
	        <exclude>tny-common-priv.h</exclude>
		<exclude>tny-simple-list-iterator-priv.h</exclude>
		<exclude>tny-simple-list-priv.h</exclude>
	</directory>
      </namespace>
    </library>
  </api>

  <api filename="libtinymailui-api.raw">
    <library name="libtinymailui-@API_VERSION@.dll">
      <namespace name="Tny.Ui">
        <dir>../../libtinymailui/</dir>
      </namespace>
    </library>
  </api>

  <api filename="libtinymailui-gtk-api.raw">
    <library name="libtinymailui-gtk-@API_VERSION@.dll">
      <namespace name="Tny.UiGtk">
	<directory path="../../libtinymailui-gtk/">
		<exclude>tny-gtk-header-list-iterator-priv.h</exclude>
		<exclude>tny-gtk-account-list-model-iterator-priv.h</exclude>
		<exclude>tny-gtk-attach-list-model-priv.h</exclude>
		<exclude>tny-gtk-attach-list-model-iterator-priv.h</exclude>
		<exclude>tny-gtk-folder-store-tree-model-iterator-priv.h</exclude>
	</directory>
      </namespace>
    </library>
  </api>

  <api filename="libtinymail-camel-api.raw">
    <library name="libtinymail-camel-@API_VERSION@.dll">
      <namespace name="Tny.Camel">
	<directory path="../../libtinymail-camel/">
		<exclude>tny-camel-account-priv.h</exclude>
		<exclude>tny-camel-imap-folder-priv.h</exclude>
		<exclude>tny-camel-queue-priv.h</exclude>
		<exclude>tny-camel-bs-mime-part-priv.h</exclude>
		<exclude>tny-camel-mime-part-priv.h</exclude>
		<exclude>tny-camel-send-queue-priv.h</exclude>
		<exclude>tny-camel-bs-msg-header-priv.h</exclude>
		<exclude>tny-camel-msg-header-priv.h</exclude>
		<exclude>tny-camel-store-account-priv.h</exclude>
		<exclude>tny-camel-bs-msg-priv.h</exclude>
		<exclude>tny-camel-msg-priv.h</exclude>
		<exclude>tny-camel-stream-priv.h</exclude>
		<exclude>tny-stream-camel.h</exclude>
		<exclude>tny-session-camel.h</exclude>
		<exclude>tny-camel-common-priv.h</exclude>
		<exclude>tny-camel-nntp-folder-priv.h</exclude>
		<exclude>tny-camel-transport-account-priv.h</exclude>
		<exclude>tny-camel-folder-priv.h</exclude>
		<exclude>tny-camel-pop-folder-priv.h</exclude>
		<exclude>tny-session-camel-priv.h</exclude>
		<exclude>tny-camel-header-priv.h</exclude>
		<exclude>tny-camel-pop-store-account-priv.h</exclude>
	</directory>
      </namespace>
    </library>
  </api>

  <api filename="@PLATFORMDIR@-api.raw">
    <library name="@PLATFORMDIR@-@API_VERSION@.dll">
      <namespace name="Tny.Platform">
       <directory path="../../@PLATFORMDIR@" />
      </namespace>
    </library>
  </api>

</gapi-parser-input>
